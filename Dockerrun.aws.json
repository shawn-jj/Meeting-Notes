{
    "AWSEBDockerrunVersion": 2,

    "containerDefinitions": [
      {
        "name": "mn-backend-aws",
        "image": "shawnjj/mn-backend:0.0.1-SNAPSHOT",
        "essential": true,
        "memory": 512,
        "portMappings": [
          {
            "hostPort": 80,
            "containerPort": 8080
          }
        ],
        "environment": [
          {
            "name": "SPRING_DATASOURCE_URL",
            "value": "jdbc:mysql://host:3306/ebdb"
          }
        ]
      }
    ]
  }